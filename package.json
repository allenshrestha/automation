{
  "name": "automation",
  "version": "1.0.0",
  "description": "Enterprise Bank Automation",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:api": "playwright test tests/api",
    "test:e2e": "playwright test tests/e2e",
    "codegen": "playwright codegen",
    "report": "playwright show-report reports/playwright",
    "metrics": "node -e \"require('./lib/core/monitor').monitor.generateReport()\"",
    "health": "playwright test tests/health", 
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rm -rf test-results reports screenshots/actual screenshots/diff",
    "setup": "npm install && npx playwright install chromium"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.40.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}